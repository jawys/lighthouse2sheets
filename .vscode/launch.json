	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${file}",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}",
			"preLaunchTask": null,
			"runtimeExecutable": "/Users/jawy/gits/lighthouse2sheets/node_modules/.bin/babel-node",
			"runtimeArgs": ["--nolazy"],
			"env": {
				"NODE_ENV": "development"
			},
			"console": "externalTerminal",
			"sourceMaps": true,
			"outFiles": []
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858,
			"address": "localhost",
			"restart": false,
			"sourceMaps": false,
			"outFiles": [],
			"localRoot": "${workspaceRoot}",
			"remoteRoot": null
		},
		{
			"name": "Attach to Process",
			"type": "node",
			"request": "attach",
			"processId": "${command:PickProcess}",
			"port": 5858,
			"sourceMaps": false,
			"outFiles": []
		}
	]
}
