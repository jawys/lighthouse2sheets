{
	"name": "lighthouse2sheets",
	"version": "0.0.1",
	"description": "Testing solution with google sheets and lighthouse",
	"main": "index.js",
	"scripts": {
		"babel-node": "babel-node",
		"dev": "set DEBUG=app:* && nodemon --exec npm run babel-node -- src",
		"build": "rm -rf dist && babel src -s -D -d dist",
		"prestart": "npm run -s build",
		"start": "node dist",
		"test": "test"
	},
	"lint-staged": {
		"*.js": [
			"prettier --write",
			"eslint --fix --quiet",
			"git add"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jawys/lighthouse2sheets.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/jawys/lighthouse2sheets/issues"
	},
	"homepage": "https://github.com/jawys/lighthouse2sheets#readme",
	"devDependencies": {
		"babel": "^6.23.0",
		"babel-cli": "^6.26.0",
		"babel-eslint": "^10.0.1",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"babel-register": "^6.26.0",
		"eslint": "^5.8.0",
		"eslint-config-airbnb-base": "^13.1.0",
		"eslint-plugin-import": "^2.14.0",
		"husky": "^1.1.2",
		"lint-staged": "^8.0.0",
		"nodemon": "^1.18.5",
		"prettier": "^1.14.3"
	},
	"dependencies": {
		"chrome-launcher": "^0.10.5",
		"googleapis": "^34.0.0",
		"lighthouse": "^3.2.1"
	}
}
